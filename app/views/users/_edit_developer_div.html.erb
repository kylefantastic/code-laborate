<%= render template: "layouts/other" %>
<div class="edit-developer-div">
  <h2>Update Information For <%=@user.first_name%></h2>
  <%= form_for @user, html: {class: "edit-developer-form", role: "form"} do |f| %>
      <div class="form-group">
          <%= f.label :first_name, "First Name" %>
            <%= f.text_field :first_name, class: "form-control" %>
          <%= f.label :last_name, "Last Name" %>
            <%= f.text_field :last_name, class: "form-control" %>
        <%= f.label :email %>
            <%= f.text_area :email, class: "form-control" %>
          <%= f.label :public_profile_url %>
            <%= f.text_area :public_profile_url, class: "form-control" %>
          <%= f.label :bootcamp %>
            <%= f.text_area :bootcamp, class: "form-control" %><br>
        <div class="actions"><%= f.submit "Update", class: "update-developer-profile" %></div>
        <%= link_to "Change your password", edit_user_registration_path %>
    </div>
  </div>
    <% end %>
    </body>
             <div class="footer">
         <footer>
            <h1>&lt;&#47;code&gt;</h1>
</footer>
</div>
